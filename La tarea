using System;
using System.Linq;
using System.Runtime;


namespace tarea
{
    class Program
    {
 
        static void Main(string[] args)     
        {
            

            var watch = new System.Diagnostics.Stopwatch();

            watch.Start();

            for (int i = 0; i < 1000; i++)
            {
                Console.Write(i);
            }

            watch.Stop();

            Console.WriteLine($"tiempo de ejecucion: {watch.ElapsedMilliseconds} ms");
        }

        static public int[] o(int[] a)
        {
           a[0] = 1;
            return a;
        }

        static public int[] t(int[] a, int m)
        {
           a[m - 1] = 0;
            return a;
        }


    }
}using System;
using System.Linq;
using System.Runtime;


